        <Text color="on-accent-muted" fontSize="sm">
          {phoneNumber}
        </Text>
      </Box>
      <Spacer />
      <IconButton
        variant="on-accent"
        aria-label="Logout"
        icon={<FiLogOut />}
        onClick={onLogout}
      />
    </HStack>
  );
};
